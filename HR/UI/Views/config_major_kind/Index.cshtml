@model IEnumerable<Model.config_major_kindModel>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
    <link href="~/Content/bootstrap.css" rel="stylesheet" />
    <link href="~/css/table.css" rel="stylesheet" />
</head>
<body>
  
        <table width="100%">
            <tr>
                <td>
                    <font color="black">
                        您正在做的业务是：人力资源--客户化设置--人力资源档案管理设置--职位分类设置
                    </font>
                </td>
            </tr>
            <tr>
                <div align="right">
                    @Html.ActionLink("新增", "Add", "", new { @class = "btn btn-primary" })
                </div>
            </tr>
        </table>
        <table  width="100%" border="1" cellpadding=0 cellspacing=1
				bordercolorlight=#848284 bordercolordark=#eeeeee
				class="TABLE_STYLE1">
            <tr>
                <th width="700" class="TD_STYLE1">
                    职位分类编号
                </th>
                <th width="700" class="TD_STYLE1">
                    职位分类名称
                </th>
                <th width="10%" class="TD_STYLE1">操作</th>
            </tr>

            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.major_kind_id)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.major_kind_name)
                    </td>
                    <td>
                     
                        @Html.ActionLink("删除", "Delete", new { id = item.Id },new { @onclick = "return confirm('亲,这样不好吧')" })
                    </td>
                </tr>
            }

        </table>

</body>
</html>
